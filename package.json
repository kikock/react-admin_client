{
  "name": "react-admin_client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^3.26.12",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "安装serve": "yarn ",
    "编码测试, 自动编译打包刷新(live-reload)": "react-scripts start",
    "生产环境打包": "react-scripts build",
    "静态服务端运行build文件夹": "serve build",
    "下载andt组件库": "yarn add antd",
    "下载依赖模块": "yarn add react-app-rewired customize-cra babel-plugin-import",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "编码测试, 自动编译打包刷新(加载config-owerrides)": "react-app-rewired start",
    "生产环境打包(加载config-owerrides)": "react-app-rewired build",
    "test(加载config-owerrides)": "react-app-rewired test",
    "eject(加载config-owerrides)": "react-app-rewired eject",
    "安装eslint": "yarn add eslint-plugin-import --dev",
    "eslint代码规范检查": "yarn run test-eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.19.0"
  }
}
